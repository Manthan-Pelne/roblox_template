    "concurrently": "^9.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-minify": "^1.0.0",
    "express-minify-html-2": "^2.0.0",
    "express-session": "^1.18.0",
    "mongoose": "^8.6.1",
    "nodemon": "^3.1.4",
    "nunjucks": "^3.2.4",
    "slugify": "^1.6.6",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.342",
    "@iconify/tailwind4": "^1.0.6"
  }
